Class {
	#name : #GtPharoMethodPartDefinition,
	#superclass : #Object,
	#instVars : [
		'id',
		'sourceCode'
	],
	#category : #'GToolkit-Pharo-Metamodels-Method - Definitions'
}

{ #category : #accessing }
GtPharoMethodPartDefinition >> id [
	^ id
]

{ #category : #accessing }
GtPharoMethodPartDefinition >> id: anObject [
	id := anObject
]

{ #category : #accessing }
GtPharoMethodPartDefinition >> sourceCode [
	^ sourceCode
]

{ #category : #accessing }
GtPharoMethodPartDefinition >> sourceCode: anObject [
	sourceCode := anObject
]
